{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "app/*"
      ],
      "@/components/*": [
        "app/components/*"
      ],
      "@/screens/*": [
        "app/screens/*"
      ],
      "@/services/*": [
        "app/services/*"
      ],
      "@/types/*": [
        "app/types/*"
      ],
      "@/utils/*": [
        "app/utils/*"
      ],
      "@/stores/*": [
        "app/stores/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}